    "@navikt/react-json-view-compare": "2.2.0",
    "@tanstack/react-query": "5.85.6",
    "axios": "1.12.0",
    "date-fns": "4.1.0",
    "dayjs": "1.11.18",
    "lodash": "4.17.21",
    "query-string": "9.2.2",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "7.62.0",
    "react-markdown": "10.1.0",
    "react-router-dom": "7.8.2",
